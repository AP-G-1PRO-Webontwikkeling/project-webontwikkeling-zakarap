
<%- include("partials/header") %>

<% for(let player of players) { %>

  <section class="row d-flex flex-row mb-3">
      <article class="card">
          <img src="<%= player.imageUrl %>" class="card-img-top">
          <h5 class=" card-title"> <a class="name-title" href="/detail/<%= player.id %>"> <%= player.name %> </a></h5>
          <ul>
            <li><%= player.position  %></li>
            <li><%= player.age %></li>
            <li><%= player.birthDate %></li>
            <li><%= player.rating %></li>

            <li><%= player.team.name %></li>
          </ul>
        </article>  
  </section>
<% } %>



<%- include("partials/footer") %>